
<script>
  var sections = <%= raw sectionsJSON %>;
  var currentSection = <%= raw currentSectionJSON %>;
  var id = currentSection ? currentSection.id : null;
  var edit = <%= raw edit %>;
  var stl = new SectionTreeList();
  stl.init(sections, id, edit);
  stl.render();
  $(document).on("click", ".section-arrow", function() {
    stl.render();
  });
  console.log(stl);
</script>